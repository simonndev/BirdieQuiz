{
  "name": "birdie-quiz",
  "version": "0.0.1",
  "description": "Build a Quiz application using Angular4, Typescript and Webpack.",
  "main": "index.js",
  "scripts": {
    "start": "npm run server:dev",
    "server:dev": "npm run webpack-dev-server -- --config webpack.dev.js --open --color --progress --profile --watch --content-base src/",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonndev/BirdieQuiz.git"
  },
  "keywords": [
    "angular4",
    "typescript",
    "webpack"
  ],
  "author": "Simon Nguyen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simonndev/BirdieQuiz/issues"
  },
  "homepage": "https://github.com/simonndev/BirdieQuiz#readme",
  "dependencies": {
    "@angular/common": "^4.1.3",
    "@angular/compiler": "^4.1.3",
    "@angular/core": "^4.1.3",
    "@angular/forms": "^4.1.3",
    "@angular/http": "^4.1.3",
    "@angular/platform-browser": "^4.1.3",
    "@angular/platform-browser-dynamic": "^4.1.3",
    "@angular/router": "^4.1.3",
    "core-js": "^2.4.1",
    "ie-shim": "^0.1.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.0",
    "zone.js": "^0.8.11"
  },
  "devDependencies": {
    "@types/node": "^7.0.27",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.3",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "ng-router-loader": "^2.1.0",
    "node-sass": "^4.5.3",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.5",
    "to-string-loader": "^1.1.5",
    "ts-node": "^3.0.4",
    "tslib": "^1.7.1",
    "tslint": "^5.4.2",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.3.4",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  }
}
